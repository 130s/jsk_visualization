<library path="lib/libjsk_rviz_plugins">
  <class name="jsk_rviz_plugin/AmbientSound"
         type="jsk_rviz_plugin::AmbientSoundDisplay"
         base_class_type="rviz::Display">
    <description>
      Displays from hark_msgs/HarkPower message
    </description>
  </class>

  <class name="jsk_rviz_plugin/Footstep"
         type="jsk_rviz_plugin::FootstepDisplay"
         base_class_type="rviz::Display">
    <description>
      Displays for jsk_footstep_sgs/FootstepArray message
    </description>
  </class>

  <class name="jsk_rviz_plugin/PolygonArrayDisplay"
         type="jsk_rviz_plugin::PolygonArrayDisplay"
         base_class_type="rviz::Display">
    <description>
      Displays for jsk_pcl_msgs/PolygonArray message
    </description>
  </class>

  
  <class name="jsk_rviz_plugin/PublishTopic"
         type="jsk_rviz_plugin::PublishTopic"
         base_class_type="rviz::Panel">
    <description>
      A panel widget to publish a topic.
    </description>
  </class>

  <class name="jsk_rviz_plugin/CancelAction"
         type="jsk_rviz_plugin::CancelAction"
         base_class_type="rviz::Panel">
    <description>
      A panel widget to cancel action
    </description>
  </class>

  <class name="jsk_rviz_plugin/SelectPointCloudPublishAction"
         type="jsk_rviz_plugin::SelectPointCloudPublishAction"
         base_class_type="rviz::Panel">
    <description>
      A panel widget to SelectPointCloudPublish action
    </description>
  </class>
  
</library>
